{
  "name": "aws-csv-to-json",
  "description": "technical assesment",
  "version": "1.0.0",
  "main": "index.js",
  "author": "vicentemanzano6@hotmail.es",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -p webpack serverless:package",
    "deploy:terraform": "BUILD_STAGE=development bash setup-scripts/terraform-deploy.sh",
    "deploy:serverless": "BUILD_STAGE=development bash setup-scripts/serverless-deploy.sh",
    "deploy:production-terraform": "BUILD_STAGE=production bash setup-scripts/terraform-deploy.sh",
    "deploy:production-serverless": "BUILD_STAGE=production bash setup-scripts/serverless-deploy.sh",
    "deploy:staging-terraform": "BUILD_STAGE=staging bash setup-scripts/terraform-deploy.sh",
    "deploy:staging-serverless": "BUILD_STAGE=staging bash setup-scripts/serverless-deploy.sh",
    "destroy": "BUILD_STAGE=development bash setup-scripts/destroy.sh",
    "destroy:production": "BUILD_STAGE=production bash setup-scripts/destroy.sh",
    "destroy:staging": "BUILD_STAGE=staging bash setup-scripts/destroy.sh",
    "plan": "BUILD_STAGE=development bash setup-scripts/plan.sh",
    "plan:production": "BUILD_STAGE=production bash setup-scripts/plan.sh",
    "plan:staging": "BUILD_STAGE=staging bash setup-scripts/plan.sh",
    "serverless": "serverless --config infrastructure/serverless/serverless.yml",
    "serverless:invoke": "AWS_SDK_LOAD_CONFIG=1 IS_OFFLINE=1 npm run serverless -- invoke local --function",
    "serverless:bundle": "npm run serverless -- webpack --out build/server",
    "serverless:offline": "AWS_SDK_LOAD_CONFIG=1 npm run serverless -- offline start",
    "serverless:package": "npm run serverless -- package",
    "setup": "bash setup-scripts/setup.sh",
    "start": "source setup-scripts/variables/development.sh && BUILD_STAGE=development npm-run-all -p watch:** serverless:offline webpack-dev-server",
    "webpack": "webpack --config infrastructure/webpack/webpack.config.client.js",
    "webpack-dev-server": "webpack serve --config infrastructure/webpack/webpack.config.client.js"
  },
  "dependencies": {
    "aws-sdk": "^2.858.0",
    "aws-serverless-express": "^3.4.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",  
    "lodash": "^4.17.21",
    "nocache": "^2.1.0"
  },
  "devDependencies": {  
    "clean-webpack-plugin": "^3.0.0",    
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^2.28.7",
    "serverless-associate-waf": "^1.2.1",
    "serverless-content-encoding": "^1.1.0",
    "serverless-layers": "^2.3.3",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-custom-domain": "^2.0.4",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.3.5",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2",
    "webpack-subresource-integrity": "^1.5.2",
    "yaml-loader": "^0.6.0"
  }
}
